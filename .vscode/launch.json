{
  "version": "0.2.0",
  "configurations": [
          {
          "name": "Launch file",
          "type": "chrome",
          "request": "launch",
          "url": "http://localhost:8888/settlers.html",
          "webRoot": "${workspaceRoot}\\run\\"
      },
      {
          "name": "Launch localhost",
          "type": "chrome",
          "request": "launch",
          "sourceMaps": true,
          "url": "http://localhost/settlers.ts/run/settlers.php#0,0,0",
          "webRoot": "${workspaceRoot}/run"
      },
      {
            "name": "Launch index.html (disable sourcemaps)",
            "type": "chrome",
            "request": "launch",
            "sourceMaps": false,
          "url": "http://localhost/settlers.ts/run/settlers.php"
        }
  ]
}